<blanks1>:
    size_hint: None, None
    x: dp(16)
    y: dp(1)
    size: dp(1), dp(1)
<MDRB@MDRaisedButton>:
    md_bg_color: .059,.49,.76,1
    theme_text_color: "Custom"
    text_color: 1,1,1,.9999
    #size_hint: 0.3,.05
    #line_color: 1, 1, 1, 1
    font_style:"Subtitle2"
    font_name: 'assets/Bodoni Bold'
    font_size: "20sp"
    size_hint: .45, .5
<MDTXT@MDTextField>:
    size_hint: 0.4,.1
    pos_hint: {'center_x':0.5 , 'center_y':0.6}
    #required: True
    font_size: "14sp"
    text_color: 0,0,0,1
    bold:True
    halign: "center"
    line_color_focus: 1, 0, 1, 1

<Connect>:
    canvas.before:
        Color:
            rgba:(.96,.87,.30,.8)
        Rectangle:
            size:self.size
            pos: self.pos
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: 'Access Screen'
            type: "top"
            right_action_items: [["close", lambda x: root.onback()]]
            elevation:10
            bold: True
            anchor_title: "center"
        MDScreen:
            MDLabel:
                text: "Please Fill the form bellow to procced"
                pos_hint: {'center_x':0.5 , 'center_y':0.9}
                halign:"center"
                bold: True
            MDLabel:
                halign: "center"
                theme_text_color: "Custom"
                text_color:0.80,0.35,0.18,1
                pos_hint: {'center_x':0.5 , 'center_y':0.78}
                id:constat
                font_size: "14sp"
                bold: True
            MDTXT:
                id:Ip
                hint_text:'IP Adress'
                focus:True
                on_text_validate:Port.focus=True
            MDTXT:
                id:Port
                hint_text:'Port'
                pos_hint: {'center_x':.5,'center_y':0.46}
                max_text_length: 5
                on_text_validate:user.focus=True
            MDTXT:
                id:user
                hint_text:'username'
                pos_hint: {'center_x':.5,'center_y':0.36}
                on_text_validate:pswd.focus=True
            MDTXT:
                id:pswd
                hint_text:'Password'
                pos_hint: {'center_x':.5,'center_y':0.26}
                password:True
                on_text_validate:root.connect_db()
            MDRaisedButton:
                md_bg_color:0,0,0,1
                theme_text_color: "Custom"
                text_color: 0.9,1,1,1
                size_hint: 0.45, 0.07
                pos_hint: {'center_x':0.5 , 'center_y':0.12}
                text:"Connect"
                font_name: 'assets/Bodoni Bold'
                font_size: "18sp"
                on_press:root.connect_db()
                line_color: 1, 1, 1, 1
<MainScreen>:
    canvas.before:
        Color:
            rgba: 0,0,0,0
        Rectangle:
            size:self.size
            pos: self.pos
    BoxLayout:
        orientation:'vertical'
        spacing: '10'
        MDToolbar:
            title: 'DataBases Management'
            type:"top"
            elevation:15
            halign:'center'
            anchor_title: "center"
        blanks1:
        blanks1:
        RelativeLayout:
            BoxLayout:
                orientation:'horizontal'
                size_hint:1,.5
                id:lab1
                padding: 10
                spacing: '10'
                pos_hint:{'center_x':.6,'center_y':.8}
                RelativeLayout:
                    MDRB:#________"Choose DataBase"
                        text:" Select Database"
                        pos_hint:{'center_x':.15,'center_y':.3}
                        on_press:
                            root.upload_db()
                    MDRB:#______"Crypt DataBase"
                        text:" Crypt Database "
                        pos_hint:{'center_x':.65,'center_y':.3}
                        on_press:
                            root.crypt_db()
            BoxLayout:
                orientation:'horizontal'
                pos_hint:{'center_x':.6,'center_y':.5}
                id:lab2
                size_hint:1,0.5
                padding: 10
                spacing: '10'
                RelativeLayout:
                    MDRB:    #_____"Send Database"
                        text:" Send Database "
                        pos_hint:{'center_x':.15,'center_y':.3}
                        on_press :root.send_db()
                    MDRB:     #"Operations"
                        text:" Operations "
                        pos_hint:{'center_x':.65,'center_y':.3}
                        increment_width: "20dp"
                        on_press : 
                            root.operations()
        #*****************************************************
        BoxLayout:
            orientation:'vertical'
            size_hint: 1,1
            padding: 10,10
            spacing: '10'
            RelativeLayout
                ProgressBar:
                    id : my_bar
                    min: 0
                    max: 100
                    value:0
                    pos_hint: {'center_x':0.5 , 'center_y':1.22}
                    size_hint :0.8,0.20
                MDLabel:
                    pos_hint: {'center_x':0.5 , 'center_y':1.1}
                    multiline: True
                    id:datashow
                    size_hint_y: None
                    theme_text_color: "Custom"
                    halign : "center"
                    size_hint : 0.7, 0.3
                    font_name: 'assets/Helvetica-Bold.ttf'
                    font_size: "16sp"
                    text_color: 0, 0, 1, 1
                MDRB:     #"Show DataTables"
                    pos_hint: {'center_x':0.5 , 'center_y':0.83}
                    size_hint : 0.46, 0.22
                    text:"Show DataTables"
                    font_size: "18sp"
                    on_press: root.showdt()

        AnchorLayout:
            theme_text_color: "Primary"
            text_color: 1,0,0,1        
            anchor_x: 'center'
            anchor_y: 'top'
            RelativeLayout
                ScrollView:
                    do_scroll_x: False
                    do_scroll_y: True
                    pos_hint: {'center_x':.84 , 'center_y':1.1}
                    MDList:
                        id: container
                        size_hint:0.3,None
                        padding: dp(-15)
                        spacing: dp(13)
                        default_size: None, dp(140)
                        default_size_hint: .1, None
                        size_hint_y: None
                        height: self.minimum_height
                        anchor_x: 'center'
                        anchor_y: 'top'
                blanks1:
                MDRB:#________"Crypt Column"
                    text:"Crypt Column"
                    size_hint :0.46,0.22
                    pos_hint: {'center_x':.5 , 'center_y':.5}
                    on_press: root.cryptcolumn()
    RelativeLayout:
        ImageButton:
            size_hint :0.2 , 0.1
            id:imgbtn
            source:'assets/logout_hover.png'if imgbtn.hovered else "assets/logout_nothover.png"
            on_press:root.log_out()
            pos_hint: {'center_x':0.5 , 'center_y':0.04}

<ShowDataTable> :
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: 'Show Data'
            type:"top"
            right_action_items: [["home", lambda x: root.onback()]]
            elevation:15        
        BoxLayout:
            id:sdtleft
            size_hint :1,.8
            padding: 20,10

<OperationsScreen>:
    clickedbtn:""#var to define clicked button

    MDScreen:
        MDLabel:
            text:'Crypted Columns'
            md_bg_color:app.theme_cls.primary_color
            halign:'center'
            size_hint_y:0.08
            size_hint_x:0.6
            pos_hint:{'center_x':.5,'center_y':.8}
            line_color: 1, 1, 1, 1
            font_style:"Subtitle2"
            font_name: 'assets/Bodoni Bold'
            font_size: "18sp"
            bold: True
    BoxLayout:
        orientation:"vertical"
        size_hint : 0.2,0.6
        pos_hint:{'center_x':.5,'center_y':.48}
        canvas:
            Color:
                rgba: 0,0,0,0.005
            Rectangle:
                pos: self.pos
                size: self.size

        ScrollView:
            halign:'center'
            do_scroll_x: False
            do_scroll_y: True
            MDList:
                pos_hint:{'center_x':.5,'center_y':.6}
                id: container2
                size_hint :1,0.4
                halign:'center'
        ScrollView:
    BoxLayout:
        orientation:'horizontal'
        size_hint : 1,0.6
        MDScreen:
            BoxLayout:
                orientation:'vertical'
                padding:10
                spacing:10
                size_hint : 0.8,0.6
                pos_hint:{'center_x':0.5,'center_y':0.4}
                MDLabel:
                    text: "Selected column"
                    valign :"middle"
                    size_hint : 0.8,0.2
                MDLabel:
                    text: "Operation"
                    size_hint : 0.8,0.2
                MDLabel:
                    text: "Elapsed Time"
                    valign :"middle"
                    size_hint : 0.8,0.2
                MDLabel:#lresult
                    text: " Result"
                    valign :"middle"
                    size_hint : 0.8,0.2
        MDScreen:
            BoxLayout:
                orientation:'vertical'
                padding:10
                spacing:5
                size_hint : 1,0.6
                pos_hint:{'center_x':.35,'center_y':.42}
                MDTextField:
                    id:showchecked
                    readonly: True
                    text:""
                    halign: 'center'
                    valign: 'middle'
                    font_name: 'assets/Helvetica.ttf'
                MDTextField:
                    id:op
                    readonly: True
                    text:root.clickedbtn
                    halign: 'center'
                    valign: 'middle'  
                    font_name: 'assets/Helvetica.ttf'
                MDTextField:
                    readonly: True
                    text:""
                    id : ltime
                    halign: 'center'
                    valign: 'middle'
                    font_name: 'assets/Helvetica.ttf'                                    
                MDTextField:
                    id: lresult
                    readonly: True
                    text:""
                    multiline:True
                    halign: 'center'
                    valign: 'middle'
                    font_name: 'assets/Helvetica.ttf'                    
    MDNavigationDrawer :
        id : nav_drawer
        orientation: 'vertical'
        padding: "10dp","10dp"
        spacing: '10dp'
        size_hint:0.4,1
        RelativeLayout:
            MDRB: # Sum
                text:"Sum"
                md_bg_color:.965,.325,.078,1
                pos_hint: {'center_x':0.5 , 'center_y':0.6}
                size_hint: 0.76,0.16
                font_size: "18sp"
                font_name: 'assets/Helvetica-Bold.ttf'
                on_press:
                    root.sumf()
                    root.clickedbtn="Sum"
            MDRB:#AVG
                text:"AVG"
                md_bg_color:.486,.733,.0,1
                pos_hint: {'center_x':0.5 , 'center_y':0.33}
                size_hint: 0.76,0.16
                font_size: "18sp"
                font_name: 'assets/Helvetica-Bold.ttf'
                on_press:
                    root.avgf()
                    root.clickedbtn="AVG"

        ScrollView:
            canvas.before:
                Color:
                    rgba:(0,0,0,.015)
                Rectangle:
                    size:self.size
                    pos: self.pos
            RelativeLayout
                MDLabel:
                    pos_hint: {'center_x':0.5 , 'center_y':0.9}
                    size_hint: 0.7,0.2
                    text: "Product"
                    theme_text_color: "Custom"
                    text_color: 0, 0, 0, 1
                    halign : "center"
                    font_name: 'assets/Bodoni Bold'
                    font_size: "22sp"                    
                MDRB:
                    text:"Russ Mul"
                    md_bg_color:0,.631,.945,1
                    pos_hint: {'center_x':0.5 , 'center_y':.7}
                    size_hint: 0.76,0.16
                    font_size: "20sp"
                    on_press:
                        root.mulru()
                        root.clickedbtn="Ru Multiplication"
                MDRB:
                    text:"Log Mul"
                    md_bg_color:1,.733,0,1
                    pos_hint: {'center_x':0.5 , 'center_y':.45}
                    size_hint: 0.76,0.16
                    font_size: "20sp"
                    on_press:
                        root.mulog()
                        root.clickedbtn="Log Multiplication"
                MDRB:
                    text:"Egy Mul"
                    md_bg_color:.01,.01,.01,1
                    pos_hint: {'center_x':0.5 , 'center_y':.2}
                    size_hint: 0.76,0.16
                    font_size: "20sp"
                    on_press:
                        root.mulegy()
                        root.clickedbtn="Egypt Multiplication"
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: 'Operations'
            type: "top"
            left_action_items: [['menu', lambda x: nav_drawer.set_state('toggle')]]
            right_action_items: [["home", lambda x: root.onback()]]
            elevation:15
        BoxLayout:                        
<RootWidget>:
    Connect:
        name:"connect"
    MainScreen:
        name:"main_screen"
    ShowDataTable:
        name:"show_data_table"
        id:sdt
    OperationsScreen
        name:"operations_screen"
